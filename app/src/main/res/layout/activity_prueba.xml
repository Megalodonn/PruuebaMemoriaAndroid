<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Prueba">


    <TextView
        android:id="@+id/textView"
        android:layout_width="68dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:text="Correctas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ScorePlayerL3"
        android:layout_width="13dp"
        android:layout_height="22dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="82dp"
        android:layout_height="22dp"
        android:layout_marginTop="16dp"
        android:text="Incorrectas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/failureesPlayerL3"
        android:layout_width="22dp"
        android:layout_height="23dp"
        android:layout_marginTop="16dp"
        android:text="0"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/iv_1l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reverso" />

    <ImageButton
        android:id="@+id/iv_2l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="52dp"
        app:layout_constraintStart_toEndOf="@+id/iv_1l3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reverso" />

    <ImageButton
        android:id="@+id/iv_3l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/reverso" />

    <ImageButton
        android:id="@+id/iv_4l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_1l3"
        app:srcCompat="@drawable/reverso"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/iv_5l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toEndOf="@+id/iv_4l3"
        app:layout_constraintTop_toBottomOf="@+id/iv_2l3"
        app:srcCompat="@drawable/reverso"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/iv_6l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toEndOf="@+id/iv_5l3"
        app:layout_constraintTop_toBottomOf="@+id/iv_3l3"
        app:srcCompat="@drawable/reverso"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/iv_7l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_4l3"
        app:srcCompat="@drawable/reverso"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/iv_8l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/iv_7l3"
        app:layout_constraintTop_toBottomOf="@+id/iv_5l3"
        app:srcCompat="@drawable/reverso" />

    <ImageButton
        android:id="@+id/iv_9l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/iv_8l3"
        app:layout_constraintTop_toBottomOf="@+id/iv_6l3"
        app:srcCompat="@drawable/reverso" />

    <ImageButton
        android:id="@+id/iv_10l3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_8l3"
        app:srcCompat="@drawable/reverso"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/startL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="152dp"
        android:background="#787777"
        android:onClick="startl3"
        android:text="Start"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_7l3"
        app:rippleColor="#8A8686"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/exitL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:onClick="exitl3"
        android:text="Salir"
        app:layout_constraintStart_toEndOf="@+id/startL3"
        app:layout_constraintTop_toBottomOf="@+id/iv_10l3"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/restartL3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="152dp"
        android:onClick="restartl3"
        android:text="Reiniciar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toEndOf="@+id/exitL3"
        app:layout_constraintTop_toBottomOf="@+id/iv_9l3"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>